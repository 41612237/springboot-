<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
<!--    配置连接数据库用的驱动  -->
    <classPathEntry location="/Users/tomgaooo/java/maven/repository/mysql/mysql-connector-java/8.0.17/mysql-connector-java-8.0.17.jar" />

<!--    配置数据库连接-->
    <context id="DB2Tables" targetRuntime="MyBatis3">

<!--        复写时,不在追加 xml 内容-->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>

<!--       context的子标签有顺序,顺序不能乱 -->
        <commentGenerator>
            <!--            不要生成文件中的注释-->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/miaosha?characterEncoding=utf-8&amp;useSSL=false"
                        userId="root"
                        password="Aa123456">
        </jdbcConnection>

<!--        是否强制BigDecimals-->
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

<!--        DO 存放位置-->
        <javaModelGenerator targetPackage="com.tomgao.miaoshabaisc.dataobject" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapping"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.tomgao.miaoshabaisc.dao"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

